{
    "name": "printful/php-gettext-cms",
    "description": "CMS backend for Gettext translation management",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-gettext": "*",
        "gettext/gettext": "v4.5.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "mockery/mockery": "^1.1",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Printful\\GettextCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Printful\\GettextCms\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/briedis/Gettext.git"
        }
    ],
    "minimum-stability": "stable"
}
