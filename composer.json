{
    "name": "printful/php-gettext-cms",
    "description": "CMS backend for Gettext translation management",
    "type": "library",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-gettext": "*",
        "gettext/gettext": "^4.8.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "mockery/mockery": "^1.6",
        "mikey179/vfsstream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Printful\\GettextCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Printful\\GettextCms\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "stable"
}
